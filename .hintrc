{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "highest-available-document-mode": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-width: none",
          "overscroll-behavior",
          "overscroll-behavior: none"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-valid-attr": "off"
      }
    ]
  }
}